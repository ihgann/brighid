<div class="row">
  <h2 class="title"><%= @course.name %></h2>
  <div class="large-4 columns right">
    <% if not @course.instructor.empty? %>
      <p style="margin-top: 20px; margin-bottom: -10px; text-align: right;">Professor <%= @course.instructor %></p> 
    <% end %>
  </div>
</div>
<hr />

<p>Notes <%= link_to "+", new_note_path(:course => @course.id) %></p>
<ul>
<% @notes.each do |note| %>
  <li><%= link_to note.name, note %></li>
<% end %>
</ul>

